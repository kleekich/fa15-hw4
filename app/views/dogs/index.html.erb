<h1>Dogs List<h1>

<% @dogs.each do |d| %> <!-- Iterates through each user under the variable u -->
  Name: <p> <%= link_to d.name, dog_path(d) %> </p> <!-- Show the users name-->
  Age : <p> <%= d.age %> </p> <!-- Show the users email -->
<% end %>
<%= link_to "new", "dogs/new" %> <!-- Create a link that appears as "new" that leads to users_new_path -->
